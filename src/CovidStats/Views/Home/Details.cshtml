@model IEnumerable<CovidStats.logic.Reports.DTO.ReportData>;

@{
    ViewData["Title"] = "Region details";
}

<div class="text-center">
    <p class="h3">Top ten @Model?.ToList()?.FirstOrDefault()?.RegionName provinces with most COVID-19 cases</p>

    <table class="table table-hover">
        <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Province</th>
            <th scope="col">Cases</th>
            <th scope="col">Deaths</th>
        </tr>
        </thead>
        <tbody>
        @foreach (var data in Model)
        {
            <tr>
                <th scope="row">@data.Id</th>
                <td>@data.ProvinceName</td>
                <td>@data.Cases</td>
                <td>@data.Deaths</td>
            </tr>
        }
        </tbody>
    </table>

</div>